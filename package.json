{
  "name": "booktracker",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.0",
    "typescript": "^5.8.3"
  },
  "version": "1.0.0",
  "description": "Book tracking application with PostgreSQL database",
  "main": "src/index.ts",
  "dependencies": {
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/swagger": "^0.8.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.42.0",
    "drizzle-zod": "^0.7.1",
    "elysia": "^0.8.17",
    "pg": "^8.11.3",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist",
    "start": "bun run dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "books",
    "tracker",
    "postgresql",
    "drizzle",
    "bun",
    "elysia"
  ],
  "author": "",
  "license": "ISC",
  "trustedDependencies": [
    "bun-types"
  ],
  "engines": {
    "bun": ">=1.0.25"
  }
}
